#1
n = int(input("숫자를 입력하시오"))
sum = 0
i = 1

while i <= n:
    sum += i
    i += 1
print(sum)




#2
n = int(input("숫자를 입력하시오"))
sum = 0

while n <= 100 : 
  sum = sum + n
  n = n + 1 
print(sum)




#3
n = int(input("자연수를 입력하시오"))

if n < 0 :
  print("잘못된 입력입니다.")
else :
  for i in range(1 , 11) : 
    print(n * i)





#5
sum = 0

while True:
  num = int(input("숫자를 입력하시오"))
  if num == 0:
    break
  else:
    if num%3 != 0 and num%5 != 0:
        sum = sum +1

print(sum)
