1
        "x = input (\"정수를 입력하시오\")\n",
        "x = int(x)\n",
        "\n",
        "if (x == 0) :\n",
        "  print(\"zero\")\n",
        "elif (x > 0) :\n",
        "  print (\"plus\")\n",
        "else :\n",
        "  print(\"minus\")\n"
      
2
        "x = input(\"영문 대문자를 입력하시오\")\n",
        "x = str(x)\n",
        "\n",
        "if x == \"A\" :\n",
        "  print(\"Excellent\")\n",
        "elif x == \"B\" :\n",
        "  print(\"Good\")\n",
        "elif x == \"C\" :\n",
        "  print(\"Usually\")\n",
        "elif x == \"D\" :\n",
        "  print(\"Effort\")\n",
        "elif x == \"F\" :\n",
        "  print(\"Failure\")\n",
        "else :\n",
        "  print(\"error\")"


