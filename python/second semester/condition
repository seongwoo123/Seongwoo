        "\n",
        "x = input(\"Number? \")\n",
        "x =  int(x)\n",
        "\n",
        "if x == 1:\n",
        "  print (\"dog\")\n",
        "elif x == 2:\n",
        "  print(\"cat\")\n",
        "elif x == 3:\n",
        "  print(\"chick\")\n",
        "else :\n",
        "  print (\"I don't know \")\n",
        "\n",
        "\n",
